{
  "name": "stratomex_js",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/stratomex_js",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_links": "*",
    "caleydo_provenance": "*",
    "caleydo_vis": "*",
    "caleydo_vis_lineup": "*",
    "caleydo_d3": "*",
    "wrapper_bootstrap_fontawesome": "*",
    "clue": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "view",
          "id": "stratomex",
          "file": "stratomex",
          "location": "center"
        },
        {
          "type": "view",
          "id": "stratomex-lineup",
          "file": "lineup",
          "location": "bottom"
        },
        {
          "type": "actionFactory",
          "id": "stratomex-note",
          "file": "notes",
          "factory": "createCmd",
          "creates": "(addStratomeXNote|changeStratomeXNote|removeStratomeXNote)"
        },
        {
          "type": "actionFactory",
          "id": "stratomex-column",
          "file": "Column",
          "factory": "createCmd",
          "creates": "(createStratomeXColumn|removeStratomeXColumn|swapStratomeXColumns|setStratomeXColumnOption|changeStratomeXColumnVis|showStratomeXInDetail)"
        },
        {
          "type": "actionCompressor",
          "id": "stratomex-compressSwap",
          "file": "Column",
          "factory": "compressSwap"
        },
        {
          "type": "actionCompressor",
          "id": "stratomex-compressCreateRemove",
          "file": "Column",
          "factory": "compressCreateRemove"
        },
        {
          "type": "actionCompressor",
          "id": "stratomex-compressHideShowDetail",
          "file": "Column",
          "factory": "compressHideShowDetail"
        },
        {
          "type": "app",
          "name": "Stratomex.js"
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "jquery": "*"
      }
    },
    "products": [
      {
        "name": "StratomeX",
        "type": "python",
        "app": "stratomex_js",
        "plugins": ["stratomex_js", "caleydo_data_mongo", "caleydo_data_redis", "caleydo_data_hdf"]
      }
    ],
    "requirejs-config": {
      "shim": {
        "webcola": {
          "deps": [
            "d3"
          ],
          "exports": "cola"
        }
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
